syntax "go" "\.go$"

TYPE:     "\<((u)?int(8|16|32|64)?|float(32|64)|byte|string|interface|nil|bool)\>"
FUNCTION: "\<func\s*\w+"
KEYWORD:  "\<(package|import|const|var|type|struct|func)\>"
KEYWORD:  "\<(for|range|if|else|case|default|switch|continue|break|return)\>"
BOOLEAN:  "\<(true|false)\>"
OPERATOR: "[-+/*=<>!~%&|^]|:="
STRING:   "\"(\\.|[^\\\"])*\"|'(\\.|[^\\'])*'"
COMMENT:  "(^|\s)//.*"
COMMENT:  start="/\*" end="\*/"
+LINT
