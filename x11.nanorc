syntax "xdefaults" "Xdefaults$"

TYPE:     "^[[:alnum:]]+\*"
VARIABLE: "\*[[:alnum:]]+\:"
NUMBER:   "\<[0-9]+\>"
OPERATOR: "[*:=]"
+BOOLEAN
+HASHCOMMENT
+LINT

syntax "xmodmap" "Xmodmap$"

KEYWORD:  "\<(add|clear|keycode|keysym|remove)\>"
NUMBER:   "\<[0-9]+\>"
OPERATOR: "="
COMMENT:  "^!.*$"
+LINT
