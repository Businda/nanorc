syntax "markdown" "\.(md|mkd|mkdn|markdown)$"

color green " _[^_]*_| \*[^*]*\*"
color brightgreen " __[^_]*__| \*\*[^*]*\*\*"

STRING:   "^>.*"
SECTION:  "^====(=*)|^----(-*)|^#.*"
NUMBER:   "^[\*+-] |^[0-9]+\. "
VARIABLE: "\[.*\]\(.*\)"
VARIABLE: "\[[^]]*\](\s*\[[^]]*\])?"
VARIABLE: "^\[.*\]:\s*.*"
VARIABLE: "!\[.*\]\(.*\)"
VARIABLE: start="<" end=">"
KEYWORD:  "`[^`]*`|^ {4}.*"

+LINT
