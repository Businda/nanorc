syntax "markdown" "\.(md|mkd|mkdn|markdown)$"

KEYWORD:  "(^|\s)_[^_]+_|(^|\s)\*[^*]+\*|(^|\s)__[^_]+__|(^|\s)\*\*[^*]+\*\*"
SECTION:  "^====(=*)|^----(-*)|^#.*"
NUMBER:   "^[\*+-] |^[0-9]+\. "
VARIABLE: "\[.*\]\(.*\)"
VARIABLE: "\[[^]]*\](\s*\[[^]]*\])?"
VARIABLE: "^\[.*\]:\s*.*"
VARIABLE: "!\[.*\]\(.*\)"
VARIABLE: start="<" end=">"
STRING:   "`[^`]*`|^ {4}.*"
COMMENT:  "^>.*"

+LINT
