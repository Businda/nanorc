syntax "markdown" "\.(md|mkd|mkdn|markdown)$"

FUNCTION: "(^|\s)_[^_]+_|(^|\s)\*[^*]+\*|(^|\s)__[^_]+__|(^|\s)\*\*[^*]+\*\*"
STRING:   "^>.*"
SECTION:  "^====(=*)|^----(-*)|^#.*"
NUMBER:   "^[\*+-] |^[0-9]+\. "
VARIABLE: "\[.*\]\(.*\)"
VARIABLE: "\[[^]]*\](\s*\[[^]]*\])?"
VARIABLE: "^\[.*\]:\s*.*"
VARIABLE: "!\[.*\]\(.*\)"
VARIABLE: start="<" end=">"
KEYWORD:  "`[^`]*`|^ {4}.*"

+LINT
